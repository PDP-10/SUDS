RADIX 8
>;ITS
;HERE FOR CONNECTOR PINS

CONMAP:	AOS (P)
	LDB TT,%CNPG		;PANEL AND GROUP NUMBER
	JUMPE TT,CPOPJ		;ILLEGAL PANEL NO
	PUSH P,TT+1
	SOS TT
	IDIVI TT,NGRPS
	CAIL TT,MXPNL
	JRST [	POP P,TT+1
		POPJ P,]			;TOO BIG A PANEL NUMBER
	MOVEI T,MXPNL-1
	SUBM T,TT		;0 FOR PANEL <MXPNL>
	IMULI TT,PNLOFT		;OFFSET BETWEEN PANELS
	MOVE T,TT+1
	POP P,TT+1
	IMULI T,GXOFST
	HRLZ T,T	;XY OF GROUP
	ADD T,[JXOFST,,JYOFST]	;LOC OF J2
	ADD T,TT		;ADD IN PANEL OFFSET
	LDB TT,%CNJK
	SKIPG TT
	ADDI T,JYOFS1		;ADD OFFSET FOR JACK 1
	LDB TT,%CONP		;PIN #
	JUMPE TT,CPOPJ
	CAILE TT,=26		;REASONABLE # OF PINS
	POPJ P,
	ADD T,FLXPIN-1(TT)
	SETZB TT,TTT
	JRST CPOPJ1

;THE ROUTINES WHICH CALL MAPIT

PINBIT:	PUSHJ P,MAPIT
	JFCL			;DIP ERROR
	SETZ TT,		;CONNECTOR ERROR
	POPJ P,			;HAPPINESS

PINMAP:	PUSHJ P,MAPIT
	JRST PINERR
	JRST CONERR
	JRST CPOPJ1

CONERR:	LDB T,%CNPG		;PANEL NO.
	PUSH P,T+1
	SOS T
	IDIVI T,NGRPS
	AOS T
	AOS T+1
	EXCH T+1,(P)
	PUSHJ P,NUMPNT
	POP P,T		;CONNECTOR GROUP
	AOS T
	OUTCHR N2L(T)
	OUTCHR ["J"]
	PUSH P,TT
	LDB TT,%CNJK
	AOS TT
	PUSHJ P,NUMPNT
	OUTCHR ["-"]
	LDB T,%CONP
	PUSHJ P,NUMPNT		;PIN
CONER1:	OUTSTR [ASCIZ / MAPPING ERROR
/]
	POP P,TT
	POPJ P,
		
PINERR:	LDB T,%DIPPNL		;PANEL NO.
	PUSHJ P,NUMPNT
	LDB T,%DIPG		;FORM A#(#)
	OUTCHR N2L(T)
	LDB T,%DIPS		;SLOT #
	PUSHJ P,NUMPNT
	OUTCHR["("]
	LDB T,%DIPP		;PIN #
	PUSHJ P,NUMPNT
	OUTCHR [")"]
	JRST CONER1

MAPRC:	HLRZ TT,T		;(1,1) IS DIP IN UPPER LEFT HAND CORNER
	SOJL TT,CPOPJ
	CAML TT,NROWS
	POPJ P,
	HRRZS T			;ROW IN TT, COL IN T
	SOJL T,CPOPJ
	CAML T,NCOLS		;(0,0) IS NOW IN UPPER LEFT
	POPJ P,
	IDIVI TT,NGRPS*GRPCOL	;FUDGE RIGHT THING INTO TT FOR MPG21
	ADDI TT,1
	LSH TT,3
	PUSH P,TT
	MOVE TT,TTT
	IDIVI TT,GRPCOL
	ADDI TT,1
	IOR TT,(P)
	LSH TT,14
	POP P,(P)
	SUBI TTT,UMLROW-1	;TRANSLATE
	MOVNS TTT		;REFLECT
	IMULI T,UMLROW
	ADDI T,1(TTT)
	IOR T,TT		;OR IN GROUP
	JRST CPOPJ1

MAPPAD:	ADDI T,1		;T HAS CONNECTOR #, STARTING AT 1 FOR FIRST
	LSH T,14
	ADDI T,(TT)		;TT HAS CONNECTOR PIN, STARTING AT 1
	POPJ P,


FPWR:	TLZ A,PLUS1!PLUS2!77
	SKIPGE A
	PUSHJ P,FPGC
	TLO A,MAPPWR
	POPJ P,

FGND:	TLZ A,PLUS1!PLUS2!77
	SKIPGE A
	PUSHJ P,FPGC
	TLO A,MAPGND
	POPJ P,

FPGC:	LDB T,[POINT 5,A,22]
	SOS T
	IDIVI T,NGRPS
	AOS T
	AOS TT
	DPB T,[POINT 3,A,20]
	DPB TT,[POINT 3,A,23]
	LDB T,[POINT 12,A,35]
	SOS T
	IDIVI T,=13
	MOVE T,[	REPEAT 4,<2>
			REPEAT 5,<3>
			REPEAT 4,<4>](TT)
	DPB T,[POINT 12,A,35]
	HRRZ A,A
	POPJ P,


PGPRTM:	SETOM PMINUS#
	CAIA
PGPRNT:	SETZM PMINUS
	JUMPL A,PGCPNT		;JUMP FOR CONNECTOR
	LDB T,%DIPPNL		;PANEL NO.
	PUSHJ P,DECOUT
	LDB T,%DIPG		;LETTER PART OF SLOT LOCATION
	PUTBYT @N2L(T)
	MOVEI T,2		;TWO DIGITS FOR SLOT #
	MOVEM T,NDIG
	LDB T,%DIPS
	PUSHJ P,NDECOUT		;SLOT #
	PUTBYT "("
	TLNN A,MAPPWR!MAPGND
	JRST NPGPNT
	TLNN A,MAPPWR
	SKIPA T,["G"]
	MOVEI T,"V"
	PUTBYT @T
PGPN1:	PUTBYT ")"
	POPJ P,

NPGPNT:	LDB T,%DIPP		;PIN #
	SKIPN PMINUS
	JRST NPGPN1
	TLNE A,PLUS1
	JRST [	PUSHJ P,DECOUT
		PUTBYT "-"
		JRST PGPN1]
NPGPN1:	TLNE A,PLUS1
	SUBI T,1		;SUBTRACT 1 FOR 14 PIN DIP #
	PUSHJ P,DECOUT
	TLNE A,PLUS1
	PUTBYT "+"
	TLNE A,PLUS2
	PUTSTR [ASCIZ /+2??????????/]
	JRST PGPN1

;PRINTS CONNECTOR PIN

PGCPNT:	LDB T,%CNPG	;PANEL NO.AND GROUP
	PUSH P,T+1
	SOS T
	IDIVI T,NGRPS
	AOS T
	AOS T+1
	EXCH T+1,(P)
	PUSHJ P,DECOUT
	POP P,T
	AOS T
	PUTBYT @N2L(T)
	PUTBYT "J"
	LDB T,%CNJK
	PUSHJ P,DECOUT
	PUTBYT "-"
	LDB T,%CONP
	JRST DECOUT

RAYGEN:	JUMPL A,RAYRP1
	TLNN A,MAPPWR!MAPGND	;ONLY DO GENERATED POWER AND GROUNDS HERE
	JRST RAYRP3		;PALM OFF THE REST ON RAYDIP!
	LDB T,%DIPPNL
	PUSHJ P,DECOUT
	LDB T,%DIPG
	PUTBYT @N2L(T)
	MOVEI T,2
	MOVEM T,NDIG
	LDB T,%DIPS		;SLOT #
	PUSHJ P,NDECOUT
	PUTSTR [ASCIZ /  /]	;2 SPACES
	MOVEI T,2		;ASSUME NOT POWER OR GROUND
	MOVEM T,NDIG
	LDB T,%DIPP
	PUTBYT "0"
	MOVEI T,"V"
	TLNN A,MAPPWR
	MOVEI T,"G"
	PUTBYT (T)
	POPJ P,

RAYRP3:	SETZM T		;CLEAR BODY BITS (14 TO 16 PIN CONVERSION ALREADY DONE
					;IN PNAMEP (WLROUT) CROCK!!!)
	JRST RAYDIP

RAYRP1:	OUTSTR [ASCIZ /CONNECTOR BECAME GENERATED PIN
/]
	JRST ERRET

VGCON:	POPJ P,

FOR NAME (GNDCLR,WAGGND,GNDOUT,VCCOUT)
<NAME:	OUTSTR [ASCIZ /NAME NOT IMPLEMENTED FOR THE MPG21 BOARD
/]
	JRST ERRET
>
BEND MPG21
>;MWL
