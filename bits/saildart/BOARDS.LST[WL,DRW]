1)		CAMN T,SMALL		;ADJACENT PINS?
1)		JRST [	ADD T,POSTMG	;YES, ADD BARE WRAP ONLY
1)			JRST DSTIS1 ]
1)		ADD T,POSTMG
1)		ADD T,WRAPMG
1)		MOVEI TTT,=500		;SET TTT TO INCREMENT FOR WIRES FROM 3 TO 8 INCHES
1)		CAILE T,=8000		;OVER 8" ?
1)		MOVEI TTT,=1000		;YES, SET TTT TO INCREMENT FOR WIRES OVER 8 INCHES
1)		IDIV T,TTT
1)		SKIPE TT
1)		ADDI T,1
