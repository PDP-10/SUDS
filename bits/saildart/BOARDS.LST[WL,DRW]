**** File 1) BOARDS.FAI[WL,DRW], Page 20 line 26
1)		MOVM T,T		;THIS IS Y PART.
1)		JUMPE G,DSTCLX		;IF THERE IS NO DISTANCE TO END OF DIP,
1)		CAMG T,NEXTR		; OR IF PINS ARE ALONG A ROW BORDER (Y=NEXTR)
1)		JRST DSTCLX		; THEN END-AROUND FUDGE ISN'T NEEDED
1)		SKIPE LNGRID		;EAF IS EASIER IF LNGRID IS TRUE
1)		JRST [	LDB C,[POINT 6,(P),5]	;GET ONE COLUMN LETTER
1)			LDB D,[POINT 6,A,23]	;GET OTHER COLUMN LETTER
1)			CAMN C,D	;SAME COLUMN?
1)			ADD T,G		;YES, ADD FUDGE
1)			JRST DSTCLX]
1)		LDB C,[POINT 12,(P),17]
**** File 2) BOARD1.FAI[WL,DRW], Page 20 line 26
2)		MOVM T,T		;THIS IS Y PART
2)		JUMPE G,DSTCLX
2)		CAIG T,=250		;IF BIGGER THAN THIS, THEN MIGHT NEED FUDGE
2)		JRST DSTCLX
2)		LDB C,[POINT 12,(P),17]
