**** File 1) BOARDS.FAI[WL,DRW], Page 8 line 1
1)	;PUT PIN ON END OF BODY LOCATION
1)	MDPC,<
1)	BPINPN:
1)	MWL,<^BPINPN_BPINPN>
1)		CHKBRD
1)		PUTBYT "("
**** File 2) BOARD1.FAI[WL,DRW], Page 7 line 94
2)	; routine to set the fraction resolution, called by XFRACTION command
2)	MWL,<
2)	^^STFRAC:	
2)		OUTSTR [asciz \Current fraction resolution is 1/\]
2)		MOVE	T,FRACTN
2)		PUSHJ	P,DECOUT		; output number
2)		OUTSTR [asciz /
2)	/]
2)	REFRACT:
2)		OUTSTR	[asciz \New resolution is 1/\]
2)	;	read number into T, skip-return if none given
2)		MOVE	T,[PUSHJ P,TTYIN]
2)		MOVEM	T,GETCHR
2)		PUSHJ	P,DECIN
2)		JFCL
2)		CAIE	TTT,12		; ended with LF?
2)		JRST	ERRET		; no, ???
2)		JUMPE	A,CPOPJ		; 0, ignore
2)		TRNE	A,1		; even value?
2)		JRST	[outstr [asciz \Don't be silly, nobody cuts wires in those lengths!
2)	\]
2)			  JRST  REFRACT]
2)	; we have an even size, so hope for best
2)		MOVEM	A,FRACTN		; store it
2)		POPJ	P,
2)	>; MWL
2)	;PUT PIN ON END OF BODY LOCATION
2)	MDPC,<
2)	^BPINPN:CHKBRD
2)		PUTBYT "("
***************


