1)	NOSTAN,<CAILE T,=8000		;LONGER THAN 8 INCHES?
1)		ADDI T,=250		;YES, ADD 1/4 INCH
1)		CAME T,SMALL		;DON'T ADD WRAPMG IF PINS ARE ADJACENT
1)		ADD T,WRAPMG		;INSULATED WIRE TO WRAP AROUND POST
1)		ADD T,POSTMG		;BARE WIRE TO WRAP AROUND POST
1)		IDIV T,SMALL		;MODULO SMALLEST POSSIBLE SPACING
1)	ITS,<	ASH TT,1
1)		CAML TT,SMALL	>	;ITS LIKES THE WELL ROUNDED APPROACH
1)	NOITS,<	SKIPE TT	>	;OVERFLOW?
1)		ADDI T,1		;YES, ADD 1
