**** File 1) BOARDS.FAI[WL,DRW], Page 10 line 10
1)		LDB TT,[POINT 6,A,5]
**** File 2) BOARD1.FAI[WL,DRW], Page 10 line 10
2)	;*************************KLUGE HACK*********************
2)	;SPECIAL HACKS FOR FAULT TOLERANT COMPUTER  W946 BOARD
2)		PUSH P,T				;GET A REGISTER
2)		LDB TT,[POINT 6,A,17]			;GET LOCATION NUMBER
2)		SUBI TT,1				;NORMALIZE IT
2)		IMULI TT,=10				;SHIFT LEFT BY 1 DECADES
2)		LDB T,[POINT 6,A,5]			;GET THE LOCATION LETTER (A=1,B=2,etc.)
2)		TRC T,7					;INVERT THE COLUMNS
2)		ADD TT,T				;ADD IN THE 1,2,...,6.
2)		DPB TT,[POINT 12,A,17]			;STORE IT BACK
2)		POP P,T					;RETURN REGISTER
2)	;************************END KLUGE HACK******************
2)		LDB TT,[POINT 6,A,5]
***************


