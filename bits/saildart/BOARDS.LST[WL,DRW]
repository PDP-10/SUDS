**** File 1) BOARDS.FAI[WL,DRW], Page 17 line 49
1)	RDLLN:	PUSHJ P,CHKLET	;LOWER TO UPPER CASE
1)		POPJ P,
1)		SKIPG CHRREG,L2N-"A"(CHRREG)	;LEGAL LETTER HERE?
1)		POPJ P,				;NO, FLUSH
1)		DPB CHRREG,[POINT 12,DESTIN,29]	;ASSUME ITS ONE LETTER
1)		GETNUM
1)		JUMPN NUMREG,CKCR		;NUMBER TYPED?
1)		PUSHJ P,CHKLET			;NO, LEGAL LETTER?
1)		POPJ P,				;NO, FLUSH
1)		LDB TT,[POINT 6,DESTIN,29]	;YES, MOVE PREVIOUS LETTER LEFT ONE
1)		DPB TT,[POINT 6,DESTIN,23]
1)		SKIPG CHRREG,L2N-"A"(CHRREG)	;NEW LETTER LEGAL?
1)		POPJ P,				;NO, FLUSH
1)		DPB CHRREG,[POINT 6,DESTIN,29]	;YES, NESTLE IT NEXT TO PREVIOUS LETTER
1)		GETNUM				;NEXT CHARACTER
1)		JUMPE NUMREG,CPOPJ		;COMPLAIN IF ITS NOT A NUMBER
1)	CKCR:
**** File 2) BOARD1.FAI[WL,DRW], Page 17 line 48
2)	RDLLN:	PUSHJ P,CHKLET
2)		POPJ P,
2)		SKIPG CHRREG,L2N-"A"(CHRREG)
2)		POPJ P,
2)		DPB CHRREG,[POINT 12,DESTIN,29]
2)		GETNUM
2)		JUMPN NUMREG,CKCR
2)		PUSHJ P,CHKLET
