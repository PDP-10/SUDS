**** File 1) BOARDS.FAI[WL,DRW], Page 7 line 41
1)	GETBRD:
1)	MWL,<^GETBRD_GETBRD>
1)	MDPC,<	TLNN M,DSKACT!MACACT	>
1)		OUTSTR [ASCIZ /BOARD TYPE?/]
1)		PUSHJ P,TREADU				;DOES CASE CONVERSION
1)		    POPJ P,				;ALTMODE
1)	MDPC,<	    JRST [	MOVE B,BRDTYP		;NULL
1)				OUTSTR @BNAMES(B)
1)				POPJ P,]
1)	>;MDPC
1)	MWL,<	    POPJ P,	>
1)		PUSHJ P,FNDBRD				;FIND THAT BOARD TYPE
1)		 JRST [	PUSHJ P,PUTFS
1)			OUTSTR [ASCIZ /BOARD NOT FOUND
1)	/]
**** File 2) BOARD1.FAI[WL,DRW], Page 7 line 40
2)	^GETBRD:
2)	MDPC,<	TLNN M,DSKACT!MACACT	>
2)		OUTSTR [ASCIZ /Board type?/]
2)		PUSHJ P,TREADU				;DOES CASE CONVERSION
2)		    POPJ P,				;ALTMODE
  1) BOARDS.FAI[WL,DRW] and 2) BOARD1.FAI[WL,DRW]	5-16-79 20:30	pages 7,7

2)		    JRST [	MOVE B,BRDTYP		;NULL
2)				OUTSTR [ASCIZ  /Board presently in use is  /]
2)				OUTSTR @BNAMES(B)
2)	OUTSTR [ASCIZ /
2)	Other boards presently avaliable are:/]		;AED	TO PRINT OUT AVALIABLE BOARDS
2)				MOVE B,[XWD -NBRDS,BNAMES]
2)			AGAIN:	OUTSTR [ASCIZ/	/]
2)				OUTSTR @(B)
2)				AOBJN B,AGAIN
2)	MWL,<			OUTSTR [ASCIZ/
2)	/]	>;MWL					;crlf
2)				POPJ P,]
2)		PUSHJ P,FNDBRD				;FIND THAT BOARD TYPE
2)		 JRST [	PUSHJ P,PUTFS
2)			OUTSTR [ASCIZ /Board not found
2)	/]
