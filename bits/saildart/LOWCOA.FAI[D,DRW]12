JAK50:	JACK(50,JAK60)
JAK60:	JACK(60,EC86)
EC86:	ECB(200,156,86,TO18.3)
TO18.3:	BODY (183,TO18.4)
	PIN (1,50,0)
	PIN (2,0,50)		;
	PIN (3,-50,0,0)
TO18.4:	BODY (184,TO5.3)
	PIN (1,50,0)
	PIN (2,0,50)
	PIN (3,-50,0)
	PIN (4,0,-50,0)
TO5.3:	BODY (53,TO5.4)
	PIN (1,100,0)
	PIN (2,0,100)
	PIN (3,-100,0,0)
TO5.4:	BODY (54,TO66.3)
	PIN (1,100,0)
	PIN (2,0,100)
	PIN (3,-100,0)
	PIN (4,0,-100,0)
TO66.3:	BODY (663,TO6631)
	PIN (1,-100,0)
	PIN (2,100,0)
	PIN (3,0,580)
	PIN (3,0,-380,0)
TO6631:	BODY (6631,.3009P)
	PIN (1,-100,0)
	PIN (2,100,0)
	PIN (3,0,-380,0)
	PIN (3,0,580)
.3009P:	BODY (30091,PIN3T)
	PIN (1,-300,0)
	PIN (2,0,-100)
	PIN (3,200,0,0)
PIN3T:	BODY (3,PIN4T)
	PIN (1,100,0)
	PIN (2,0,100)
	PIN (3,-100,0,0)
PIN4T:	BODY (4,0)
	PIN (1,50,0)
	PIN (2,0,50)
	PIN (3,-50,0)
	PIN (4,0,-50,0)
>;PCSW!LAYSW

