02300		SUBI A,LBUFL
02400	SETB:	MOVE B,INIX
02500		IDIVI B,44
02600		MOVSI B,400000
02700		MOVN C,C
02800		ROT B,(C)
02900	>;NOCMU
03000	CMU,<
