**** File 1) WLROUT.FAI[WL,DRW], Page 5 line 38
1)		SETZM LSTEMP		;FLAG NEW WINNER FOUND
1)		MOVEM B,LNGMIN		;YES, SAVE NEW RECORD
1)		MOVEM TT,LSTCND		;SAVE CANDIDATE FOR INCLUSION
1)	PHS12:	SOJGE TT,PHS13		;TRY NEXT PIN (DONE?)
1)		SOS B,PHSSW		;PERHAPS, CHECK SWITCH
1)		AOJN B,PHS14		;DONE THIS PASS?
1)		SETOM LSTEMP		;NO, HALF DONE. FLAG 1ST HALF
1)		HLRZ T,ENDS		;GET OTHER END OF LIST
1)		JRST PHS11		;REPEAT
1)	PHS14:	MOVE T,LSTCND		;GET THE WINNER
1)		IORM TTT,PNTBL(T)	;MARK IT
1)		SKIPL LSTEMP		;WINNER CONNECTED TO RIGHT END?
1)		MOVSS ENDS		;NO, MAKE IT THE RIGHT END
**** File 2) WLROU1.FAI[WL,DRW], Page 5 line 38
2)		SETZM	LSTEMP		;FLAG NEW WINNER FOUND
2)		MOVEM B,LNGMIN		;YES, SAVE NEW RECORD
2)		MOVEM TT,LSTCND	;SAVE CANDIDATE FOR INCLUSION
  1) WLROUT.FAI[WL,DRW] and 2) WLROU1.FAI[WL,DRW]	5-10-79 22:52	pages 5,5

2)	PHS12:	SOJGE	TT,PHS13	;TRY NEXT PIN (DONE?)
2)		SOS B,PHSSW		;PERHAPS, CHECK SWITCH
2)		AOJN B,PHS14		;DONE THIS PASS?
2)		SETOM	LSTEMP		;NO, HALF DONE. FLAG 1ST HALF
2)		HLRZ T,ENDS		;GET OTHER END OF LIST
2)		JRST PHS11		;REPEAT
2)	PHS14:	MOVE T,LSTCND	;GET THE WINNER
