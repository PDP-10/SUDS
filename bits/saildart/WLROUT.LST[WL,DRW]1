1)	PHS1:	SETZM ENDS		;SET BOTH ENDS OF RUN TO FIRST PIN
1)		SETOM RUNLST		;SET BOTH NEIGHBORS OF FIRST PIN TO ENDS
1)		MOVE B,[RUNLST,,RUNLST+1]	;WIPE OUT THE WHOLE RUN TABLE
1)		BLT B,RUNLST+MAXP-1
**** File 2) WLROU1.FAI[WL,DRW], Page 5 line 14
2)	PHS1:	SETZM	ENDS		;SET BOTH ENDS OF RUN TO FIRST PIN
2)		SETOM	RUNLST		;SET BOTH NEIGHBORS OF FIRST PIN TO ENDS
2)		MOVE	B,[XWD RUNLST,RUNLST+1]	;WIPE OUT THE WHOLE RUN TABLE
2)		BLT B,RUNLST+MAXP-1
***************


**** File 1) WLROUT.FAI[WL,DRW], Page 5 line 22
1)		IORM TTT,PNTBL		;MARK FIRST PIN
1)	PHS10:	HRLOI T,377777		;SET MIN LENGTH TO INFINITY
1)		MOVEM T,LNGMIN
1)		HRRZ T,ENDS		;GET ONE END OF THE RUN
1)	PHS11:	MOVEI TT,-1(A)		;INDEX THROUGH ALL PINS
1)	PHS13:	TDNE TTT,PNTBL(TT)	;THIS PIN MARKED YET?
**** File 2) WLROU1.FAI[WL,DRW], Page 5 line 22
2)		ORM	TTT,PNTBL	;MARK FIRST PIN
2)	PHS10:	HRLOI T,377777	;SET MIN LENGTH TO INFINITY
