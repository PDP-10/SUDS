**** File 1) WLROUT.FAI[WL,DRW], Page 22 line 192
1)		 JRST FNDRN1		;NO
1)		MOVE E,TTT		;SAVE FOR COROUTINE CALL
1)		MOVE D,B		; "
1)		MOVE C,A		; "
1)		PUSHJ P,PINMAP		;TRY MAPPING
1)		 JRST FNDRN1		;LOSE
1)		PUSHJ P,@FDSP		;FIND ITS CARD PIN
1)		MOVE TTT,E		;RESTORE FOR COROUTINE CALL
1)		PUSHJ P,@(P)		;COROUTINE BACK
1)	FNDRN1:	FETCH(H,H,NXTP)
