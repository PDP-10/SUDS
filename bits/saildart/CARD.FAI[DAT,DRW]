BXXX_XXX-12000			;12 INCHES OF BOARD SPACE
	XY	XXX,-5220,I
	XY	BXXX-125,-5220
	XY	BXXX-125,-2885
	BCON	-2885
	BEXY	-125,3240
	BCON	YYY+3240
	XY	BXXX-125,3910
	XY	BXXX-125,5215
	XY	XXX+125,5215
	CONT	5090
	EXY	125,-2380
	CONB	YYY-2750
	EXY	125,-2380
	CONT	YYY-2500
	EXY	125,-2380
	CONB	YYY-2750
ORG .-1
	XY	XXX,-5220
	FMARK

YYY_5090
	PADDLE(XXX,YYY,"A",1)
	PADDLE(XXX,YYY-2750,"B",1)
	PADDLE(XXX,YYY-2500-2750,"C",1)
	PADDLE(XXX,YYY-2750*2-2500,"D",1)
	BPADDLE(-2885,"F",1)
	BPADDLE(-2885+3240,"E",1)
	FMARK
	PADDLE(XXX,YYY,"A",2)
	PADDLE(XXX,YYY-2750,"B",2)
	PADDLE(XXX,YYY-2750-2500,"C",2)
	PADDLE(XXX,YYY-2750*2-2500,"D",2)
	BPADDLE(-2885,"F",2)
	BPADDLE(-2885+3240,"E",2)
	FMARK
REPEAT 2,<
	XY	BXXX-1420,-5715
	XY	BXXX-1420,5715
	XY	XXX+1420,-5715
	XY	XXX+1420,5715
	FMARK
>
	XY	0,-6000
	XY	-3000,6000
	XY	4000,6000
	CMARK

LENGTH_.-CARD

	END STRT

