BXXX_XXX-12000			;12 INCHES OF BOARD SPACE
	XY	XXX,-5220,I
	XY	BXXX,-5220
	XY	BXXX,5215
	XY	XXX+125,5215
	CONT	5090
	EXY	125,-2380
	CONB	YYY-2750
	EXY	125,-2380
	CONT	YYY-2500
	EXY	125,-2380
	CONB	YYY-2750
