;9	BEGIN INTEL CARD (8.175 X 10.5)

IFE CRDTYP-9<

NAME INTEL,< >

PRINTS/
COMPILING STANDARD INTEL CARD
/

CARD:	CRDVER
	XY	-5250,-4090,I
	XY	5250,-4090
	XY	5250,4085
	XY	-5250,4085
	XY	-5250,3005
	XY	-5480,2995
	XY	-5530,2955
	XY	-5530,-3310
	XY	-5480,-3350
	XY	-5250,-3360
	XY	-5250,-4090
	FMARK


FOR PNN_1,2			;ODD FOR COMPONENT SIDE FINGERS (FIRST PASS)
<				; EVEN FOR SOLDER SIDE FINGERS (SECOND PASS)
PN__PNN
   FOR K_2885,2510,-125
