	XY	XXX,-5220
	FMARK

YYY_5090
	PADDLE(XXX,YYY,"A",1)
	PADDLE(XXX,YYY-2750,"B",1)
	PADDLE(XXX,YYY-2500-2750,"C",1)
	PADDLE(XXX,YYY-2750*2-2500,"D",1)
	FMARK
