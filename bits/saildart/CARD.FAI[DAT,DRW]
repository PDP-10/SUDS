	XY	-4400,-5750	; BOTTOM LEFT
	XY	 4850,0		; RIGHT CENTER TARGET
	XY	-4400,5750	; TOP LEFT
	CMARK

LENGTH_.-CARD

	END STRT

>;IFE CRDTYP-22

;END OF MULTIBUS CARD
;23	BEGIN AMPEX ARM-20 CARD WITH ODD/EVEN SIDE CPINS - AMPEXX

IFE CRDTYP-23<

NAME AMPEXX

PRINTS/
COMPILING AMPEX ARM-20 CARD FOR 8136-UG159 BOARD TYPE
/

CARD:	CRDVER
	XY	   5500,-7675,I
	XY	   4900,-7675
	XY	   4900,-8075
	XY	   1200,-7975
	XY	   1200,-7675

	XY	    300,-7675
	XY	    300,-7975
	XY	 - 4800,-8075
	XY	 - 4800,-7675
	XY	 - 5500,-7675

	XY	 - 5650,-7525
	XY	 - 5650, 7450
	XY	 - 5425, 7675
	XY	   5425, 7675
	XY	   5650, 7450

	XY	   5650,-7525
	XY	   5500,-7675
	FMARK


FOR PNN_1,2			;ODD FOR COMPONENT SIDE FINGERS (FIRST PASS)
<				;EVEN FOR SOLDER SIDE FINGERS (SECOND PASS)
				;(NOTE THAT THE COMPONENT SIDE IS THE "B" SIDE!)

;EDGE CONNECTOR P1
;P1 100 PINS 0.100 SPACING (49 SPACES)
    PN__PNN
    FOR K_-4700,200,100
    <
	XY	K,-7675
	XY	K,-8475
	CCPIN	"A",PN
	PN__PN+2
    >

;EDGE CONNECTOR P2
;P2 72 PINS 0.100 SPACING (35 SPACES)
    PN__PNN
    FOR K_1300,4800,100
    <
	XY	K,-7675
	XY	K,-8475
	CPIN	"M","B",PN
	PN__PN+2
    >


    FMARK
>


REPEAT 2,<
	XY	-5000,-8475	;SHORTING BAR P1
	XY	  500,-8475

	XY	 1000,-8475	;SHORTING BAR P2
	XY	 5100,-8475
	FMARK
>

;The following holes must be put in by hand. Otherwise, you get alignment
;targets at these locations.
;	XY	 5400,-7475	;BOTTOM RIGHT HOLE
;	XY	-5400,-7475	;BOTTOM LEFT HOLE
;	XY	 5400, 7425	;TOP RIGHT HOLE AND LEVER
;	XY	-5400, 7425	;TOP LEFT HOLE AND LEVER

;Alignment targets
	XY	    0,-8675	;BOTTOM TARGET
	XY      -6650,    0	;LEFT   TARGET
	XY	    0, 8675	;TOP    TARGET
	XY       6650,    0	;LEFT   TARGET

	CMARK

LENGTH_.-CARD

	END STRT

>;IFE CRDTYP-23

;END OF AMPEXX CARD
