	XY	 3100,-6000,I
	XY	-3100,-6000
	XY	-3350,-5750
	XY	-3350,5750
	XY	-3100,6000
	XY	 3100,6000
	XY	 3100,5360
	XY	 3400,5360
	XY	 3400,2280
	XY	 3100,2280
	XY	 3100,1213
	XY	 3400,1213
	XY	 3400,-5622
	XY	 3100,-5622
	XY	 3100,-6000
	FMARK


FOR PNN_1,2			;;A ON COMPONENT SIDE
<				;;B ON SOLDER SIDE

;EDGE CONNECTOR P1
;86 PINS, 0.156 SPACING (42 SPACES)
    PN__1			;;START WITH PIN 1 ON BOTH SIDES
    FOR K_-5480,1072,156
    <
	XY	3100,K
	XY	4000,K
	CPIN	"M",<"A"+PNN-1>,PN
	PN__PN+1
    >

    FMARK
>


